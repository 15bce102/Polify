<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_practice"
    app:startDestination="@id/questionsTestFragment">

    <fragment
        android:id="@+id/questionsTestFragment"
        android:name="com.example.polify.ui.fragment.QuestionsFragment"
        android:label="fragment_questions"
        tools:layout="@layout/fragment_questions">
        <argument
            android:name="battle"
            android:defaultValue="@null"
            app:argType="com.andruid.magic.game.model.data.Battle"
            app:nullable="true" />
        <argument
            android:name="start_time"
            android:defaultValue="-1L"
            app:argType="long" />
        <argument
            android:name="battleType"
            android:defaultValue="test"
            app:argType="string" />
        <action
            android:id="@+id/action_questionsFragment_to_resultsTestFragment"
            app:destination="@id/resultsTestFragment"
            app:popUpTo="@id/questionsFragment"
            app:popUpToInclusive="true" />
    </fragment>

    <fragment
        android:id="@+id/resultsTestFragment"
        android:name="com.example.polify.ui.fragment.ResultsTestFragment"
        android:label="fragment_results_test"
        tools:layout="@layout/fragment_results_test">
        <argument
            android:name="battle_id"
            android:defaultValue="test"
            app:argType="string" />
        <argument
            android:name="score"
            android:defaultValue="0"
            app:argType="integer" />
    </fragment>
</navigation>